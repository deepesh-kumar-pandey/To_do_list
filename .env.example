# Environment Configuration Example
# Copy this file to config.js and update with your actual values

# IMPORTANT: For production deployment
# 1. Never commit config.js with real credentials to public repositories
# 2. Add config.js to .gitignore
# 3. Use your hosting platform's environment variable system

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Application Settings
APP_ENVIRONMENT=production  # development | production
APP_VERSION=1.2.0

# Feature Flags
ENABLE_BOSS_BATTLES=true
ENABLE_SHOP=true
ENABLE_ACHIEVEMENTS=true
ENABLE_SOUND_EFFECTS=false
